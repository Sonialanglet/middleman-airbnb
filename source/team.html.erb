
<h2>This template was built with <i class="fa fa-heart"></i> by</h2>

<div id="team">
  <div class="about-content">
      <ul class="list-inline text-center">
        <%  data.users.each do |user| %>
          <li>
            <%= image_tag "https://kitt.lewagon.com/placeholder/users/#{user[:github_nickname]}", class: "img-circle avatar-md" %>
            <h3><%= user[:name].capitalize %></h3>
            <p><%= user[:github_nickname].capitalize %></p>
          </li>
          <% end %>
      </ul>
  </div>
</div>


